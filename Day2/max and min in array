#include <iostream>
using namespace std;

int main() {
  int n,i,temp,arr[30];
  cin>>n;
   for(i=0;i<n;i++)
  {
      cin>>arr[i];
  }

 int max;
 max=arr[0];
 int min=arr[0];
  for(i=0;i<n;i++)
  { 
      if(max < arr[i])
      max=arr[i];
  }
  for(i=0;i<n;i++)
  { 
      if(min > arr[i])
      min=arr[i];
  }
  cout<<min<<" "<<max;
   
    return 0;
}
