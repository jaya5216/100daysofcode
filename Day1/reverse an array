// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
  int s,e,n,i,temp,arr[30];
  cin>>n;
   for(i=0;i<n;i++)
  {
      cin>>arr[i];
  }
  for(s=0, e=n-1;s<e;s++)
  {
     
          temp=arr[s];
          arr[s]=arr[e];
          arr[e]=temp;
          e--;
          
  }
  for(i=0;i<n;i++)
  {
 cout<<arr[i]<<" ";
  }
    return 0;
}
